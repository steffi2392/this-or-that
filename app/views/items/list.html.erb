
<div class="container">

	<%= render "item" %>
	
	<% i = 0 %>
	<div class="row">
		<% @items.each do |item| %>
			<ul>
				<li class = "list_image">
					<ul>
						<li><%= link_to image_tag(item.image_front), item.link, :target => "_blank" %></li>
						<li class="list_rank"><%= item.number %></li>
						
					</ul>
				</li>
			</ul>
		<% end %>
	</div>

</div>
